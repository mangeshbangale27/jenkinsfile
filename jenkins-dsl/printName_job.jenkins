pipelineJob('Print Name') {
  definition {
    cpsScm {
      scm {
        git {
          remote {
            url('https://github.com/mangeshbangale27/job-dsl-and-jenkinsfile.git')
          }
          branch('main')
        }
      }
      scriptPath('jenkins-files/print_name.jenkinsfile')
    }
  }
  parameters{
    choiceParam('VALUE', ['0','1'], 'Choose a number')
  }
}