#!/usr/bin/env groovy
@Library('shared-library@main') _

pipeline {
    agent any
    stages {
        stage(' Update confluence') {
            steps {
                script{
                        echo "invoking shared library..."
                        echo "current branch : " + BRANCH_NAME
                    updateConfluensePage ENVIRONMET : "prod", STATUS: "UP", SOURCE_BRANCH : "${BRANCH_NAME}", IMAGE_TAG : "abc-xyz"
                        echo " shared library executed..."
                    }
                }
                
        }
    }
}
