#!/usr/bin/env groovy
@Library('shared-library@main') _

pipeline {
    agent any
    environment {
        branch = "${scm.branches[0].name}"
        bname = "$BRANCH_NAME"
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'     
                echo "$branch"
                 
                echo "branch name is : $bname"
            }
        }
    }
}
