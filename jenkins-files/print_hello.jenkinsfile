@Library('shared-library@main') _

pipeline {
    agent any
    environment {
        branch = ${scm.branches[0].name}
    }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'     
                echo "$branch"
                
                script {
                def branch = env.BRANCH_NAME    
                echo "branch name is : $branch"
            }
            }
        }
    }
}
